{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "AdminUser": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "password": {
          "type": "string"
        }
      },
      "required": [
        "name",
        "email",
        "password"
      ]
    },
    "User": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "cpf": {
          "type": "string"
        },
        "phoneNumber": {
          "type": "string"
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        },
        "purchase": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Purchase"
          }
        }
      },
      "required": [
        "name",
        "email",
        "cpf",
        "phoneNumber"
      ]
    },
    "Purchase": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        },
        "type": {
          "type": "string",
          "default": "shake",
          "enum": [
            "shake",
            "comum"
          ]
        },
        "amount": {
          "type": "number"
        },
        "isGift": {
          "type": "boolean",
          "default": false
        },
        "user": {
          "$ref": "#/definitions/User"
        }
      },
      "required": [
        "amount",
        "user_id"
      ]
    }
  },
  "type": "object",
  "properties": {
    "adminUser": {
      "$ref": "#/definitions/AdminUser"
    },
    "user": {
      "$ref": "#/definitions/User"
    },
    "purchase": {
      "$ref": "#/definitions/Purchase"
    }
  }
}